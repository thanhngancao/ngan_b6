  <link rel="stylesheet" href="/css/login.css">

      <h1 class="Log">Login</h1>
      <%- include('partials/messages.ejs')%>
        <form action="/users/login" method="POST">
        <div class="form-group">
          <label for="email">Email</label>
          <input
            type="email"
            id="email"
            name="email"
            class="form-control"
            placeholder="Enter Email"
          />
        </div>
        <div class="form-group">
          <label for="password">Password</label>
          <input
            type="password"
            id="password"
            name="password"
            class="form-control"
            placeholder="Enter Password"
          />
        </div>
        <button type="submit" class="bulo">Login</button>
      </form>
      <p class="noacc">If you don't have account, you could register <a href="/users/register">Register</a></p>
      
      <div class="image">
        <img class="image1 n" src="images/cantons.gif" alt="" id="h1" onclick="Change1();">
        <img class="image2 n" src="images/Switzerland-village_1369729.jpg" alt="" id="h2" onclick="Change2();">
        <img class="image3 n" src="images/food.jpg" alt="" id="h3" onclick="Change3();">
      </div>
    
